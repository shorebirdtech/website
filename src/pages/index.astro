---
import { config } from '../config';
import { Divider } from '../components/Divider';
import { FAQ } from '../components/FAQ';
import { Footer } from '../components/Footer';
import { Hero } from '../components/Hero';
import { StableBanner } from '../components/StableBanner';
import { Navbar } from '../components/Navbar';
import { NewsLetter } from '../components/NewsLetter';
import { Pricing } from '../components/Pricing';
import { ProductsCodePush } from '../components/ProductsCodePush';
import { PromoVideo } from '../components/PromoVideo';
import { ScrollToTopButton } from '../components/ScrollToTopButton';
import { Team } from '../components/Team';
import { Customers } from '../components/Customers';
import MainLayout from '../layouts/MainLayout.astro';

import '../styles/Theme.css';
import '../styles/Shapes.css';
---

<MainLayout title={config.app}>
  <Navbar links={config.navLinks} client:load />
  <StableBanner client:load />
  <Hero client:load />
  <ProductsCodePush client:load />
  <PromoVideo client:load />
  <Divider />
  <Pricing client:load />
  <Team client:load />
  <Customers client:load />
  <NewsLetter client:load />
  <FAQ client:load />
  <Footer />
  <ScrollToTopButton client:load />
</MainLayout>
